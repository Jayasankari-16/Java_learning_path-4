class Codechef
 {
 	public static void main (String[] args) throws java.lang.Exception
 	{
 		Scanner sc=new Scanner(System.in);
 		int t=sc.nextInt();
 		while(t-->0){
 		    int n=sc.nextInt();
 		    int a[]=new int[n];
 		    int m=n;
 		    for(int i=0;i<n/2;i++){
 		        a[i]=m;
 		        a[n-i-1]=m-1;
 		        m=m-2;
 		    }
 		    if(n%2!=0){
 		        a[n/2]=1;
 		    }
 		    for(int i=0;i<n;i++){
 		        System.out.print(a[i]+" ");
 		    }
 		    System.out.println("");
 		}
 	}
 }
